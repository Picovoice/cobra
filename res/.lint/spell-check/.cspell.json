{
    "language": "en",
    "dictionaries": [
      "dict"
    ],
    "dictionaryDefinitions": [
      {
        "name": "dict",
        "path": "./dict.txt",
        "addWords": true
      }
    ],
    "ignorePaths": [
      // submodules
      "../../../demo/c/dr_libs/**/*",
      "../../../demo/c/pvrecorder/**/*",
  
      // binaries
      "**/*.a",
      "**/*.dll",
      "**/*.dylib",
      "**/*.jar",
      "**/*.mp3",
      "**/*.node",
      "**/*.ppn",
      "**/*.pv",
      "**/*.so",
      "**/*.wasm",
      "**/wasm/**",
      "**/dist/**",
      "**/lib/ios/**/*",
      "**/*.wav",
      "../../../lib/ios/**/*",
  
      // android
      "**/AndroidManifest.xml",
      "**/activity_main.xml",
  
      // ios
      "**/*.xcconfig",
      "**/*.podspec",
      "**/Podfile",
      "**/*.plist",
      "**/contents.xcworkspacedata",
      "**/*.storyboard",
      "**/*.pbxproj",
      "**/*.xcscheme",
      "**/*.m",

      // java
      "**/build.gradle",
      "**/gradle-wrapper.properties",
      "**/gradle.properties",
      "**/gradlew",
      "**/gradlew.bat",
      "**/proguard-rules.pro",
  
      // javascript
      "**/package.json",
      "**/packages-lock.json",
      "**/tsconfig.json",
      "**/tslint.json",

      // wasm
      "**/lib/wasm/**/*.js",

      // mcu
      "../../../demo/mcu/stm32f411/stm32f411e-disco/Drivers/*",
      "../../../demo/mcu/stm32f411/stm32f411e-disco/Middlewares/**/*",
      "../../../demo/mcu/stm32f411/stm32f411e-disco/Startup/*",
      "../../../demo/mcu/stm32f411/stm32f411e-disco/Inc/*",
      "../../../demo/mcu/stm32f411/stm32f411e-disco/Src/*",
      "**/*.ld"
    ]
  }
